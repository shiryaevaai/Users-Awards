@model IEnumerable<EpamTask6_1.UserList.Entities.Role>

@if (Model.Count()==0)
{    
    @: No roles    
}
else
{
<table class="table">
    <tr>
        <th>
         @Html.DisplayNameFor(model => model.RoleName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RoleName) 
        </td>
    </tr>
}

</table>
}
